#include <opencv/cv.h>
#include <opencv/cv.h>
#include <opencv2/core/core.hpp>
#include <opencv2/imgproc/imgproc.hpp>
#include <opencv2/highgui/highgui.hpp>
#include <opencv2/calib3d/calib3d.hpp>
#include <opencv2/video.hpp>
#include <opencv2/stitching.hpp>
#include "DEF.H"

int main()
{



    cv::Mat img = cv::imread("OpenCV_Chessboard.png",GRAY_SCALE);

    CvSize csSize(4,4);


    bool an=findChessboardCorners(img,csSize,img,1);

	cv::Mat a;

	cv::VideoCapture cap(0);

	cap >> a;


	cv::imshow("OKNO",a);

	cv::waitKey();
	return 0;
}
